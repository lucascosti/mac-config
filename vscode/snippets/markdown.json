{
  // Place your snippets for markdown here. Each snippet is defined under a snippet name and has a prefix, body and
  // description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
  // $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the
  // same ids are connected.
  // Example:
  // "Print to console": {
  // 	"prefix": "log",
  // 	"body": [
  // 		"console.log('$1');",
  // 		"$2"
  // 	],
  // 	"description": "Log output to console"
  // }
  "Product name reusable": {
    "prefix": "product",
    "body": "{% data variables.product.${TM_SELECTED_TEXT:$CLIPBOARD} %}"
  },
  "GitHub - docs variable": {
    "prefix": "GitHub",
    "body": "{% data variables.product.prodname_dotcom %}",
    "description": "GitHub variable"
  },
  "GitHub Actions - docs variable": {
    "prefix": "Actions",
    "body": "{% data variables.product.prodname_actions %}",
    "description": "GitHub Actions variable"
  },
  "GitHub Packages - docs variable": {
    "prefix": "Packages",
    "body": "{% data variables.product.prodname_registry %}",
    "description": "GitHub Packages variable"
  },
  "Plain Code block": {
    "prefix": "code",
    "body": [
      "```",
      "$1",
      "```"
    ]
  },
  "JS code block": {
    "prefix": "codejavascript",
    "body": [
      "```javascript",
      "$1",
      "```"
    ]
  },
  "Shell code block": {
    "prefix": "codeshell",
    "body": [
      "```shell",
      "$1",
      "```"
    ]
  },
  "YAML code block": {
    "prefix": "codeyaml",
    "body": [
      "```yaml",
      "$1",
      "```"
    ]
  },
  "Note block": {
    "prefix": "note",
    "body": [
      "{% note %}",
      "",
      "**Note:** $1",
      "",
      "{% endnote %}"
    ]
  },
  "Warning block": {
    "prefix": "warning",
    "body": [
      "{% warning %}",
      "",
      "**Warning:** $1",
      "",
      "{% endwarning %}"
    ],
    "description": "Warning block"
  },
  "Raw block": {
    "prefix": "raw",
    "body": [
      "{% raw %}",
      "",
      "{% endraw %}"
    ],
    "description": "Raw block"
  },
  "Liquid if": {
    "prefix": "if ",
    "body": [
      "{% ifversion fpt or ghes > 3.1 or ghae %}something{% endif %}"
    ],
    "description": "Liquid if condition"
  }
}